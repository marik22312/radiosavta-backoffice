    "test": "node scripts/test.js",
    "snyk-protect": "snyk protect"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@svgr/webpack": "4.3.3",
    "@types/axios": "^0.14.0",
    "@types/chance": "^1.0.10",
    "@types/jest": "^26.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.41",
    "@types/react-big-calendar": "^0.24.6",
    "@types/react-dom": "^16.9.8",
    "@types/react-loader-spinner": "^3.1.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/reactstrap": "^8.2.0",
    "@types/styled-components": "^5.1.0",
